package com.ldl.ibatis.dbcp.dao.mapper;

import java.sql.Timestamp;

/**
 * 评价信息.
 * @author lidongliang
 *
 */
public class EvaluateInfo {
	
	private int evaSeq;
	private String evaMsg;
	private int parentEva;
	private int movieSeq;
	private Timestamp createTime;
	public int getEvaSeq() {
		return evaSeq;
	}
	public void setEvaSeq(int evaSeq) {
		this.evaSeq = evaSeq;
	}
	public String getEvaMsg() {
		return evaMsg;
	}
	public void setEvaMsg(String evaMsg) {
		this.evaMsg = evaMsg;
	}
	public int getParentEva() {
		return parentEva;
	}
	public void setParentEva(int parentEva) {
		this.parentEva = parentEva;
	}
	public int getMovieSeq() {
		return movieSeq;
	}
	public void setMovieSeq(int movieSeq) {
		this.movieSeq = movieSeq;
	}

	
}
