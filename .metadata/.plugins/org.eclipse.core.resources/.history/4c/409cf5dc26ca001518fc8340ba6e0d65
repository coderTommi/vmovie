<!-- 
	namespace：必须与对应的接口全类名一致
	id:必须与对应接口的某个对应的方法名一致
	
 -->
<mapper namespace="com.ldl.ibatis.dbcp.dao.mapper.EvaluateInfo">
	<insert id="save" parameterType="EvaluateInfo">
		insert into evaluateinfo(userid,aliasname,userpwd,email,gender) values(#{userId},#{aliasName},#{userPwd},#{email},#{gender})
	</insert>
	
</mapper>